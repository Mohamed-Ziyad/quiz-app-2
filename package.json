  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ],
    "*.css": [
      "pretty-quick --staged",
      "stylelint --fix",
      "git add"
    ],
    "*.scss": [
      "pretty-quick --staged",
      "stylelint --syntax=scss --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "NODE_ENV=production lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
